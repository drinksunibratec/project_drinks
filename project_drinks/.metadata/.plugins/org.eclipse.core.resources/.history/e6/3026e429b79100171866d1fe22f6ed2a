package util;

public enum Situacao {
	RASCUNHO("RASCUNHO"),
	AGUARDANDO("AGUARDANDO ATENDIMENTO"), 
	ATENDIMENTO("EM ATENDIMENTO"),
	MATERIAL("AGUARDANDO MATERIAL"),
	CONCLUIDO("CONCLUIDO");

	private String label;

	Situacao(String label){
		this.label = label;
	}

	public String getLabel(){
		return label;
	}

}
